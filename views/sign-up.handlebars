<h1 id="sign-up-h1">• Linda •</h1>
<h2 id='sign-up-h2'> welcome to Linda's hidden paradise </h2>

    <form id="sign-up" method="POST" action="/sign-up">
      <fieldset>

      <span class="form-group">
        <label for="first-name"> first name </label>
        <input class="form-control" type="first-name" name="first-name" id="first-name">
        <label for="last-name"> last name </label>
        <input class="form-control" type="last-name" name="last-name" id="last-name">
        <label for="email"> email </label>
        <input class="form-control" type="email" name="email" id="email">
      </span>
      <br>
      <span class="form-group">
          <label for="age"> age </label>
          <input class="form-control" type="age" name="age" id="age">
          <label for="city"> city </label>
          <input class="form-control" type="city" name="city" id="city">
          <label for="password">password</label>
          <input class="form-control" type="password" name="password" id="password">
      </span>

        <div class="form-group">
          <input class="btn btn-primary" type="submit" value="Sign in">
        </div>
      </fieldset>
    </form>